<!DOCTYPE html>
<html>
  <head>
    <title>TechHustle</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
    <link href="https://s3.amazonaws.com/aws-website-danielphillips-k44l4/img/favicon.ico" rel="icon" type="image/x-icon" />
  </head>

  <body>
    <% if current_user %>
    <%= current_user.username %>
    <%= link_to "Logout", logout_path, method: :delete %>
  <% else %>
  <% end %>
    <%= yield %>
  </body>
</html>
